#!/usr/bin/env sed -rf
# order by most-structured first


s@\<0x[0-9a-f]{8,}@${ADDR}@g
s@\<0x[0-9A-F]{8,}@${addr}@g


# 2021-12-14T20:05:30+00:00
s/[0-9]{4}-[0-9]{2}-[0-9]{2}[ T][0-9]{2}:[0-9]{2}:[0-9]{2}(\.[0-9]{3,9})?([-+]00:00|Z)/$(date -u -Ins)/g
# 2023-02-23 21:06:33.764093
s/[0-9]{4}-[0-9]{2}-[0-9]{2}[ T][0-9]{2}:[0-9]{2}:[0-9]{2}(\.[0-9]{3,9})?/$(date -Ins)/g


# 59fc2385-b52f-4e54-8b74-43cd00811636
s/[a-f0-9]{8}(-[a-f0-9]{4}){4}[0-f0-9]{8}/${UUID}/g

# 376b8a5f9a9f0ed87e
s/[A-F0-9]{16,}/${HASH}/g
s/[a-f0-9]{16,}/${hash}/g
s/[0-9]{8,}/${BIGINT}/g
