#!/sourceable/bash
export USER HOME PATH SHELL
USER="$(whoami)"
HOME="$(echo ~)"
if ! [[ "$PATH" ]]; then
  PATH="/sbin:/bin:/usr/sbin:/usr/bin:/usr/local/sbin:/usr/local/bin:$HOME/bin"
fi
PATH=~/.local/bin:~/private-dotfiles/bin:~/bin:"$PATH"
if ! [[ "$TERM" ]]; then
  TERM=xterm-256color
fi
if ! [[ "$SHELL" ]]; then
  export SHELL
  SHELL="$(readlink /proc/$$/exe)"
fi
